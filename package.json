{
  "name": "draft-js-vim",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "npm-run-all compile:ts compile:es compile:html compile:webpack",
    "compile:es": "babel lib/es6 --out-dir lib/es5",
    "compile:html": "mkdirp ./lib/docbase/ && cpx \"src/pages/*\" ./lib/docbase/",
    "compile:ts": "tsc --outDir lib/es6/ --target ES6",
    "compile:watch": "npm-run-all --parallel compile:watch:*",
    "compile:watch:html": "npm run compile:html -- --watch",
    "compile:watch:ts": "npm run compile:ts -- --watch",
    "compile:watch:webpack": "npm run compile:webpack -- --watch",
    "compile:webpack": "babel-node ./node_modules/webpack/bin/webpack.js",
    "start:server:dev": "nodemon lib/es5/server.js --exec babel-node",
    "start:server": "babel-node lib/es5/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "8.5.1",
    "@types/react": "16.0.30",
    "awesome-typescript-loader": "3.4.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "cpx": "1.5.0",
    "css-loader": "0.28.7",
    "draft-js": "0.10.4",
    "express": "4.16.2",
    "mkdirp": "0.5.1",
    "node-sass": "4.7.2",
    "nodemon": "1.13.2",
    "npm-run-all": "4.1.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.19.1",
    "tsc": "1.20150623.0",
    "typescript": "2.6.2",
    "webpack": "3.10.0"
  }
}
